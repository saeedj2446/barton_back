{
  "name": "wholesale-marketplace-backend",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "s": "nest start --watch --entry-file src/local-main.ts",
    "s2": "nest start --watch",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register src/main.ts'",
    "prod": "node dist/main",
    "db": "prisma db push",
    "db2": "prisma generate",
    "opendb": "prisma studio",
    "generateSeedData": "tsx prisma/seed.ts",
    "b": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "build": "nest build && npm run prisma:generate",
    "start:prod": "node dist/main",
    "prisma:db-push": "prisma db push",
    "postbuild": "npm run prisma:generate",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "start": "node dist/main",
    "debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed:units": "ts-node prisma/units_seed.ts",
    "seed:categories": "ts-node prisma/categories_seed.ts",
    "seed:specs": "ts-node prisma/specs_seed.ts",
    "seed:category-specs": "ts-node prisma/category-specs_seed.ts",
    "seed:package_seed": "ts-node prisma/package_seed.ts",
    "seed:locations": "ts-node prisma/locations_seed.ts",
    "seed:industries": "ts-node prisma/industries_seed.ts",
    "seedAll": "npm run seed:units && npm run seed:categories && npm run seed:specs && npm run seed:category-specs && npm run seed:package_seed && npm run seed:locations && npm run seed:industries"
  },
  "dependencies": {
    "@fastify/multipart": "^9.3.0",
    "@fastify/static": "^8.2.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bull": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.7",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.1.2",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.7",
    "@prisma/client": "^6.14.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bip32": "^2.0.6",
    "bip39": "^3.0.4",
    "bitcoinjs-lib": "^5.2.0",
    "bull": "^4.16.5",
    "cache-manager": "^7.2.3",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "exceljs": "^4.4.0",
    "fastify": "^5.6.1",
    "fs": "^0.0.1-security",
    "ioredis": "^5.8.2",
    "mongodb": "^6.20.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "redis": "^5.9.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "serverless-http": "^4.0.0",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "tiny-secp256k1": "^1.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.1",
    "@nestjs/schematics": "^11.0.1",
    "@nestjs/testing": "^11.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.4",
    "@types/cache-manager": "^5.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.16.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/redis": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prisma": "^6.14.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
